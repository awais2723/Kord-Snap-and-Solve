{
  "name": "kord-snap-and-solve",
  "main": "expo-router/entry",
  "version": "1.1.0",
  "description": "Kord Snap & Solve is a React Native Expo app that can easily solve mathematical questions in step by step, by uploading the image of the question to solve",
  "repository": "https://github.com/awais2723/Kord-Snap-and-Solve.git",
  "author": "AreebGhani <areebghanidps@gmail.com>, awais2723 <muhmmadawais3993@gmail.com>, HumairAbbasKhan <humairabbas923@gmail.com>",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "upgrade": "echo Expo upgrading... &&  expo upgrade",
    "fix": "echo Expo fixing... && expo install --fix",
    "build:android": "echo Android building... && npx eas-cli build -p android --profile preview --local",
    "build:ios": "echo IOS building... && npx eas-cli build -p ios --profile preview --local",
    "build:web": "echo Web building... && npx expo export --platform web -c --output-dir web-build",
    "publish": "echo EAS publishing... && npx eas-cli update",
    "publish:web": "echo Firebase deploying... && npx firebase deploy --only hosting",
    "prepare": "echo Husky preparing... && husky",
    "format": "echo Prettier formatting... && prettier --write .",
    "format:check": "echo Prettier checking... && prettier --check .",
    "lint": "echo ESlint checking... &&  eslint .",
    "lint:fix": "echo ESlint fixing... &&  eslint . --quiet --fix",
    "test": "echo Jest testing... && jest --coverage -o -u --passWithNoTests",
    "commit": "echo Git status... && git status && echo Git adding... && git add --all && echo Git commiting... && git-cz && echo Git pushing... && git push -u origin master",
    "release": "echo Version Releasing... && standard-version",
    "eject": "echo WARNING: Ejecting from Expo will permanently change your project configuration and may not be reversible. Are you sure you want to proceed? (yes/no) && read -p 'Your answer: ' answer && [[ $answer == [yY]es ]] && echo Expo ejecting... && expo eject"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "yarn format:check",
      "yarn lint",
      "yarn test"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@expo/webpack-config": "^19.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/material-bottom-tabs": "^6.2.28",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "axios": "^1.6.8",
    "expo": "^50.0.15",
    "expo-build-properties": "^0.12.1",
    "expo-constants": "^15.4.5",
    "expo-dev-client": "^3.3.11",
    "expo-font": "^11.10.3",
    "expo-linking": "^6.2.2",
    "expo-router": "^3.4.8",
    "expo-splash-screen": "^0.26.4",
    "expo-status-bar": "^1.11.1",
    "expo-system-ui": "^2.9.3",
    "expo-web-browser": "^12.8.2",
    "firebase": "10.11.0",
    "mime": "^4.0.3",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-document-scanner-plugin": "^1.0.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.6.2",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.19.10",
    "react-navigation": "^5.0.0",
    "semver": "^7.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/format": "^19.0.3",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.5",
    "@types/expo": "^33.0.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "conventional-changelog-atom": "^4.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "inquirer": "9",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.4",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "3.3.2",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.3",
    "typescript-eslint": "^7.6.0"
  }
}
